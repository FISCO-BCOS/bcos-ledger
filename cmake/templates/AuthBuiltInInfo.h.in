#pragma once
#define GET_AUTH_BIN(isSM, SM_BIN, BIN) ((isSM) ? (SM_BIN) : (BIN))
#define INTERCEPT_SM_BIN    "@AUTH_INTERCEPT_SM_BIN@"
#define INTERCEPT_BIN       "@AUTH_INTERCEPT_BIN@"
#define PROPOSAL_SM_BIN     "@AUTH_PROPOSAL_SM_BIN@"
#define PROPOSAL_BIN        "@AUTH_PROPOSAL_BIN@"
#define COMMITTEE_SM_BIN    "@AUTH_COMMITTEE_SM_BIN@"
#define COMMITTEE_BIN       "@AUTH_COMMITTEE_BIN@"

#define AUTH_INTERCEPT_BIN(isSM) (GET_AUTH_BIN((isSM), (INTERCEPT_SM_BIN), (INTERCEPT_BIN)))
#define AUTH_PROPOSAL_BIN(isSM)  (GET_AUTH_BIN((isSM), (PROPOSAL_SM_BIN), (PROPOSAL_BIN)))
#define AUTH_COMMITTEE_BIN(isSM) (GET_AUTH_BIN((isSM), (COMMITTEE_SM_BIN), (COMMITTEE_BIN)))
